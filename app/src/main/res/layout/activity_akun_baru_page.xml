<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AkunBaruPage">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FF9800">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/poppins_bold"
            android:text="New Account"
            android:textColor="@color/white"
            android:textSize="25sp" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/vectors" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="50dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="center"
                android:src="@drawable/frame" />


            <TextView
                android:id="@+id/a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="25dp"
                android:fontFamily="@font/poppins_bold"
                android:text="Full Name"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/d"
                android:layout_width="373dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/a"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="10dp"
                android:background="@color/material_dynamic_neutral20"
                android:backgroundTint="#31FF9800"
                android:paddingVertical="5dp"
                android:paddingStart="30dp"
                android:text="example@example.com"

                />

            <TextView
                android:id="@+id/e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/d"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/poppins_bold"
                android:text="Password"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/f"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/e"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="10dp"
                android:background="@color/material_dynamic_neutral20"
                android:backgroundTint="#31FF9800"
                android:paddingVertical="5dp"
                android:paddingStart="30dp"
                android:text="password" />

            <TextView
                android:id="@+id/wk1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/f"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/poppins_bold"
                android:text="Email"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/wk2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wk1"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="10dp"
                android:background="@color/material_dynamic_neutral20"
                android:backgroundTint="#31FF9800"
                android:paddingVertical="5dp"
                android:paddingStart="30dp"
                android:text="example@gmailcom" />

            <TextView
                android:id="@+id/wk3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wk2"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/poppins_bold"
                android:text="Mobile Number"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/wk4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wk3"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="10dp"
                android:background="@color/material_dynamic_neutral20"
                android:backgroundTint="#31FF9800"
                android:paddingVertical="5dp"
                android:paddingStart="30dp"
                android:text="example 085759873" />

            <TextView
                android:id="@+id/wk5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wk4"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/poppins_bold"
                android:text="Email"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/wk6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wk5"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="10dp"
                android:background="@color/material_dynamic_neutral20"
                android:backgroundTint="#31FF9800"
                android:paddingVertical="5dp"
                android:paddingStart="30dp"
                android:text="example@gmailcom" />


            <LinearLayout
                android:id="@+id/liner2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wk6"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/wr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/login"
                    android:layout_centerHorizontal="true"
                    android:fontFamily="@font/poppins_medium"
                    android:text="by countyning, you agree to" />

                <TextView
                    android:id="@+id/wr2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/login"
                    android:layout_centerHorizontal="true"
                    android:fontFamily="@font/poppins"
                    android:text="term of us and privasi policy"
                    android:textColor="#FF5722" />


            </LinearLayout>


            <Button
                android:id="@+id/login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/liner2"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:backgroundTint="#FF9800"
                android:paddingHorizontal="50dp"
                android:text="Sign Up" />

            <TextView
                android:id="@+id/h"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/login"
                android:layout_centerHorizontal="true"
                android:fontFamily="@font/poppins_medium"
                android:text="or sign up with" />

            <LinearLayout
                android:id="@+id/liner1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/h"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="8dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:layout_marginHorizontal="5dp"
                    android:src="@drawable/googleicon" />

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:layout_marginHorizontal="5dp"
                    android:src="@drawable/fbicon" />

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:layout_marginHorizontal="5dp"
                    android:src="@drawable/fingerprinticon" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/liner1"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/aa"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:fontFamily="@font/poppins_medium"
                    android:text="already have any account?"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/txt_signup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:fontFamily="@font/poppins_medium"
                    android:text="Log In"
                    android:textColor="#FF5722"
                    android:textSize="12sp" />


            </LinearLayout>


        </RelativeLayout>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>